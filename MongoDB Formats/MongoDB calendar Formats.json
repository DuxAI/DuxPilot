MongoDB calendar.events Format:
    "event_id" = STRING Format
    "event_name" = STRING Format
    "Has Changed" = BOOLEAN (FLAG) Format
    "event_description" = STRING Format
    "event_ts" = STRING Format
    "eveve_date" = STRING Format
    "eveve_duration" = INT 32 Format
    "creator_id" = STRING Format
    "participant_id_arr" = Array of STRINGs Format
        "participant_id_0" = STRING Format
        "participant_id_1" = STRING Format
        ...

MongoDB calendar.users Format:
    "user_id" = STRING Format
    "user_name" = STRING Format
    "name" = STRING Format
    "second_name" = STRING Format
